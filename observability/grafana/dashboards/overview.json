{
  "id": null,
  "title": "Overview",
  "tags": ["spring-saas-core"],
  "timezone": "browser",
  "panels": [
    {
      "title": "HTTP requests",
      "type": "graph",
      "targets": [
        {
          "expr": "rate(http_server_requests_seconds_count{job=\"spring-saas-core\"}[5m])",
          "legendFormat": "{{uri}}"
        }
      ]
    },
    {
      "title": "Tenants created",
      "type": "stat",
      "targets": [
        {
          "expr": "increase(saas_tenants_created_total[1h])",
          "legendFormat": "tenants"
        }
      ]
    },
    {
      "title": "Access denied",
      "type": "stat",
      "targets": [
        {
          "expr": "increase(saas_access_denied_total[1h])",
          "legendFormat": "denied"
        }
      ]
    }
  ],
  "schemaVersion": 16,
  "version": 1
}
